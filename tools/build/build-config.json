      "angular-version": 4,
      "bundle": {
        "globals": {
          "@angular/common": "ng.common",
          "@ngx-config/core": "_ngxConfig_core",
          "@ngx-cache/core": "_ngxCache_core"
        },
        "external": []
      },
      "runTests": true
    }
