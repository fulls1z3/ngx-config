{
  "@ngx-config": {
    "core": {
      "angularVersion": 4,
      "bundle": {
        "globals": {},
        "external": []
      },
      "runTests": true
    },
    "http-loader": {
      "angularVersion": 4,
      "bundle": {
        "globals": {
          "@angular/http": "ng.http",
          "@ngx-config/core": "_ngxConfig_core"
        },
        "external": [
          "rxjs/add/operator/map",
          "rxjs/add/operator/toPromise"
        ]
      },
      "runTests": true
    },
    "fs-loader": {
      "angularVersion": 4,
      "bundle": {
        "globals": {
          "fs": "fs",
          "@ngx-config/core": "_ngxConfig_core"
        },
        "external": []
      },
      "runTests": false
    },
    "merge-loader": {
      "angularVersion": 4,
      "bundle": {
        "globals": {
          "rxjs/Observable": "Rx.Observable",
          "lodash": "_",
          "lodash/index": "_",
